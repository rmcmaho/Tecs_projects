/**
 *  Exclusive-or gate.  out = a xor b.
 */

CHIP Xor {

    IN  a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    And(a=nota, b=b, out=topA);
    And(a=a, b=notb, out=bottomB);
    Or(a=topA, b=bottomB, out=out);
}
